{
  "name": "align-2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo-google-fonts/playfair-display": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "expo": "~54.0.33",
    "expo-font": "^14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "15.12.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.12",
    "babel-preset-expo": "~14.0.x",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.1",
    "react-test-renderer": "^19.1.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/utils/validateProfile.js",
      "src/utils/submitProfile.js",
      "src/utils/dateHelpers.js",
      "src/components/FAB.js",
      "src/components/VisibilityToggle.js",
      "src/components/StepIndicator.js"
    ],
    "coverageThreshold": {
      "src/utils/": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      },
      "src/components/": {
        "statements": 70,
        "branches": 70,
        "functions": 70,
        "lines": 70
      }
    }
  },
  "private": true
}
